<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	         http://www.springframework.org/schema/beans/spring-beans.xsd
		 http://www.springframework.org/schema/context
		 http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <bean id="car" class="com.zxx.minispringx.beans.factory.beans.Car">
        <property name="brand" value="宝马5系"></property>
    </bean>

    <bean id="person" class="com.zxx.minispringx.beans.factory.beans.Person">
        <property name="name" value="曾祥鑫"></property>
        <property name="age" value="26"></property>
        <property name="car" ref="car"></property>
    </bean>

    <bean class="com.zxx.minispringx.beans.factory.common.CustomBeanFactoryPostProcessor"/>

    <bean class="com.zxx.minispringx.beans.factory.common.CustomerBeanPostProcessor"/>
</beans>